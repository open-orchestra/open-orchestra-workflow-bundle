{
  "name": "open-orchestra/open-orchestra-workflow-function-bundle",
  "description": "A workflow function bundle for Open Orchestra",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "OpenOrchestra community",
      "homepage": "https://github.com/open-orchestra/open-orchestra-function-bundle/graphs/contributors"
    }
