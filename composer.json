  "require": {
    "open-orchestra/open-orchestra-cms-bundle": "~1.1.0",
    "open-orchestra/open-orchestra-base-bundle": "~1.1.0",
    "doctrine/data-fixtures": "1.1.1",
    "doctrine/mongodb-odm-bundle" : "3.1.0",
